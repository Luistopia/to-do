{\n    const [title, setTitle] = useState(\"\");\n    const [todos,setTodos] = useState([]);\n\n    function handleChange(e){\n       const value = e.target.value;\n       setTitle(value);\n    }\n    function handleSubmit(e){\n        e.preventDefault();\n        if (title.trim() !== \"\") {\n        const newTodo = {\n            id: crypto.randomUUID(),\n            title: title,\n            completed: false,\n        }\n        const temp = [... todos];\n        temp.unshift(newTodo);\n        setTodos(temp);\n        setTitle(' ')\n    }\n}\n\n    function handleUpdate(id, value){\n     const temp =[... todos];\n     const item= temp.find(item => item.id ===id);\n     item.title = value;\n     setTodos(temp);\n    }\n\n    function handleDelete(id){\n     const temp= todos.filter(item => item.id !== id);\n     setTodos(temp);\n    }\n    return (\n       <div className=\"todoContainer\">\n       <form className=\"todoContainerForm\" onSubmit={handleSubmit}>\n        <input onChange={handleChange} className=\"todoInput\" \n        value={title} />\n        <input onClick={handleSubmit } \n        type=\"submit\" \n        value=\" Create to do\" className=\"buttonCreate\" />\n\n \n       </form>\n\n       <div className=\"todosContainer\">\n        {\n            todos.map((item) => (\n               <Todo key={item.id} item={item} onUpdate={handleUpdate} onDelete={handleDelete}/>\n            ))\n        }\n       </div>\n    </div>\n    );\n}","/Applications/projects.2/assortedReact/projects/toDo/src/Components/Todo.js",[],[],{"ruleId":"30","severity":1,"message":"31","line":21,"column":26,"nodeType":"32","messageId":"33","endLine":21,"endColumn":27,"fix":"34"},{"ruleId":"30","severity":1,"message":"31","line":29,"column":22,"nodeType":"32","messageId":"33","endLine":29,"endColumn":23,"fix":"35"},"rest-spread-spacing","Unexpected whitespace after spread operator.","SpreadElement","unexpectedWhitespace",{"range":"36","text":"37"},{"range":"38","text":"37"},[55